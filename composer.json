{
  "name": "ccinn/kafka-swoole-core",
  "description": "Kafka-swoole's core",
  "type": "framework",
  "keywords": [
    "client",
    "kafka-swoole",
    "kafka"
  ],
  "config": {
    "platform": {
      "php": "7.1.12"
    },
    "preferred-install": "dist",
    "sort-packages": true
  },
  "require": {
    "ext-json": "*",
    "ext-sockets": "*",
    "ext-posix": "*",
    "php": "^7.1",
    "symfony/console": "^4.3",
    "symfony/dotenv": "^4.3",
    "symfony/event-dispatcher": "^4.3",
    "symfony/translation": "^4.3",
    "symfony/yaml": "^4.3",
    "monolog/monolog": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Kafka\\": "src"
    },
    "files": [
      "src/Support/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "KafkaTest\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5",
    "swoft/swoole-ide-helper": "dev-master"
  }
}
